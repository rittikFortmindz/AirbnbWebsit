<% layout('/layouts/boilerplate') -%>

    <body>
        <h3>All listings</h3>
        <form method="GET" action="/listings/new">
            <button>Create new post</button>
        </form>
        <ul>
            <% for(let listing of allListing){ %>

                <li><a href="/listings/<%= listing.id %>">
                        <%=listing.title %>
                    </a></li>
                <% }%>
        </ul>
    </body>